		"@sveltejs/kit": "^2.6.0",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.6",
		"@types/eslint": "^8.56.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "2.36.0-next.13",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.72.0",
		"svelte": "5.0.0-next.259",
