	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:s": "node dev.js",
		"build": "VITE_SSR=true vite build && cp package.json build",
		"build:spa": "VITE_SSR=false vite build && npx cap copy",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint .",
